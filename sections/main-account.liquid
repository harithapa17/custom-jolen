              </div>
              <div class="sidebar-items help-block">
                <h3>Need help</h3>
                <button class="sidebar-btn" id="online-tab">FAQ</button>
              </div>
              <div class="sidebar-call-action">
                <h4>CALL</h4>
                <p>88000778809907</p>
              </div>
              <div class="sidebar-call-action">
                <h4>EMAIL</h4>
                <p><a href="mailto:Jolenesupport@gmail.com">{%- for order in customer.orders -%}{{ checkout.email }}{% endfor %}</a></p>
              </div>
              <div class="logged-out">
                <a href=" {{ routes.account_logout_url }}">Sign Out</a>
              </div>
            </div>
<div class="customer order section-{{ section.id }}-padding">
  <svg style="display:none">
    <symbol id="icon-discount" viewBox="0 0 12 12">
      <path fill-rule="evenodd" clip-rule="evenodd" d="M7 0h3a2 2 0 012 2v3a1 1 0 01-.3.7l-6 6a1 1 0 01-1.4 0l-4-4a1 1 0 010-1.4l6-6A1 1 0 017 0zm2 2a1 1 0 102 0 1 1 0 00-2 0z" fill="currentColor">
    </symbol>
  </svg>
  {% comment %}<div>
    <h1 class="customer__title">{{ 'customer.account.title' | t }}</h1>
 
    <a href="{{ routes.account_url }}">{{ 'customer.account.return' | t }}</a>
  </div>
  {% endcomment %}

  <div>
    <div>
      {% comment %} <h2>{{ 'customer.order.title' | t: name: order.name }}</h2> {% endcomment %}
     <h2 class="order_title">Online Purchases</h2>
      {%- for order in customer.orders -%}
      <div class="order-info">
        <div class="order-status">
          <h3>Order Confirmed</h3>
        </div>
        <div class="order-aciton">
